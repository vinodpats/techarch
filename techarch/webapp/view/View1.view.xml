<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="techarch.controller.View1" displayBlock="true">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                <content>
               <List noDataText="Loading.." id="list0" items="{/results}" footerText="CopyRight- New York Times">
                    <items>
                        <StandardListItem type="Navigation" title="{title}" description="{abstract}" icon="{multimedia/0/url}" id="item0" press="onPress"/>
                    </items>
                </List>
                </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>